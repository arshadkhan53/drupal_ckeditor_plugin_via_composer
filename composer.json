{
"name": "arshadkhan53/drupal_ckeditor_plugin_via_composer",
"description": "Add ckeditor plugin via composer.",
"type": "drupal-module",
"license": "GPL-2.0-or-later",
"homepage": "https://github.com/arshadkhan53/drupal_ckeditor_plugin_via_composer",
"support": {
"issues": "https://github.com/arshadkhan53/drupal_ckeditor_plugin_via_composer"
},
"minimum-stability": "dev",
"prefer-stable": true,
"repositories": [
  {
    "type": "package",
    "package": {
      "name": "ckeditor/preview",
      "version": "4.14.1",
      "type": "drupal-library",
      "dist": {
          "url": "https://download.ckeditor.com/preview/releases/preview_4.14.1.zip",
          "type": "zip"
        },
      "require": {
        "composer/installers": "~1.0"
      }
    }
  }
  ],
"require": {
 "ckeditor/preview":  "*",
"drupal/colorbutton": "^1.2",
"drupal/panelbutton": "^1.3",
"drupal/ckeditor_layout_manager": "^1.0"
}
}
