{
"name": "arshadkhan53/drupal_ckeditor_plugin_via_composer",
"description": "Add ckeditor plugin via composer, Add this to you main composer.json file.",
"type": "drupal-module",
"license": "GPL-2.0-or-later",
"homepage": "https://github.com/arshadkhan53/drupal_ckeditor_plugin_via_composer",
"support": {
"issues": "https://github.com/arshadkhan53/drupal_ckeditor_plugin_via_composer"
},
"minimum-stability": "dev",
"prefer-stable": true,
"repositories": {
"colorbutton": {
"type": "package",
"package": {
"name": "ckeditor/colorbutton",
"version": "4.14.0",
"type": "drupal-library",
"extra": {
"installer-name": "colorbutton"
},
"dist": {
"url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.14.1.zip",
"type": "zip"
},
"require": {
"composer/installers": "~1.0"
}
}
},
"panelbutton": {
"type": "package",
"package": {
"name": "ckeditor/panelbutton",
"version": "4.14.0",
"type": "drupal-library",
"extra": {
"installer-name": "panelbutton"
},
"dist": {
"url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.14.1.zip",
"type": "zip"
},
"require": {
"composer/installers": "~1.0"
}
}
},
"preview": {
"type": "package",
"package": {
"name": "ckeditor/preview",
"version": "4.14.0",
"type": "drupal-library",
"extra": {
"installer-name": "preview"
},
"dist": {
"url": "https://download.ckeditor.com/preview/releases/preview_4.14.1.zip",
"type": "zip"
},
"require": {
"composer/installers": "~1.0"
}
}
}
},
"require": {
"ckeditor/colorbutton": "*",
"ckeditor/panelbutton": "*",
"ckeditor/preview": "*",
"drupal/colorbutton": "^1.2",
"drupal/panelbutton": "^1.3",
"drupal/ckeditor_layout_manager": "^1.0"
}
}
